var date =  new Date(value);
var y = 1900+date.getYear();
var m = "0"+(date.getMonth()+1);
var d = "0"+date.getDate();
var hous = "0"+date.getHours();
var mins = "0"+date.getMinutes();
var secs = "0"+date.getSeconds();
return y+"-"+m.substring(m.length-2,m.length)+"-"+d.substring(d.length-2,d.length)+" "+hous.substring(hous.length-2,hous.length)+":"+mins.substring(mins.length-2,mins.length)+":"+secs.substring(secs.length-2,secs.length);
